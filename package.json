{
  "name": "think-study",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:up": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:up",
    "migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "migrate:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "syncstagingtolocal": "tsx syncstagingtolocal.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "^3.22.0",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "^3.22.0",
    "@payloadcms/plugin-search": "^3.22.0",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "embla-carousel-react": "^8.5.2",
    "file-type": "18.5.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "^15.1.5",
    "next-sitemap": "^4.2.3",
    "payload": "latest",
    "payload-admin-bar": "^1.0.6",
    "peek-readable": "5.0.0",
    "prism-react-renderer": "^2.3.1",
    "radix-ui": "^1.1.2",
    "react": "^19.0.0",
    "react-calendly": "^4.3.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.45.4",
    "react-icons": "^5.5.0",
    "sharp": "0.32.6",
    "ssh2": "^1.16.0",
    "strtok3": "7.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/aos": "^3.0.7",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@types/react-icons": "^2.2.7",
    "@types/ssh2": "^1.15.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "overrides": {
    "strtok3": "7.0.0",
    "peek-readable": "5.0.0",
    "file-type": "18.5.0"
  }
}
